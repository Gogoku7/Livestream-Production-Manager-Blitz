@if (AppState.FormatSelection.SelectedFormat == null) return;

@if (AppState.FormatSelection.SelectedFormat == Enums.FormatsEnum.Singles)
{
    <LPMBlitz.FG.Components.SetForms.ManageSinglesSet Singles="AppState.Singles"></LPMBlitz.FG.Components.SetForms.ManageSinglesSet>
}
else if (AppState.FormatSelection.SelectedFormat == Enums.FormatsEnum.Doubles)
{
    <LPMBlitz.FG.Components.SetForms.ManageDoublesSet></LPMBlitz.FG.Components.SetForms.ManageDoublesSet>
}